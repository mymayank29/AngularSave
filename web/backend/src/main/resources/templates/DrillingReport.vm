<html>
<head>
    <!--<link rel="stylesheet" type="text/css" href="sample.css">-->
    <style>
    body {

    }

    * {
    margin: 0;
    padding: 0;
    border-spacing: 0px;
    }

    .header{
        font-weight: bold;
    	width: 100%;
    }

    .header .logo{
    	float: left;
    	border: 0px solid black;
    	height: 100px;
    	width: 90px;
    }

    .header .logo img{
    	height:95%;
    	width:95%;
    }

    .header .title{
    	float: center;
    	text-align: center;
    	border: 0px solid black;
    }

    .page {
    	border: 1px solid black;
    }

    td {
    	border: 1px solid black;
    }

    .page {
    	table-layout: fixed;
    	width: 100%;
    	padding: 3px;
    }

    .header {
    	border: 0px solid black;
    	border-spacing: 0px;
    	padding: 0px;
    	margin: 0px;
    }

    .leftSide, .rightSide {
    	border: 1px solid black;
    	border-spacing: 0px;
    	padding: 0px;
    	margin: 0px;
    }

    tbody {
    	border-spacing: 0px;
    }

    .leftSide {
    	table-layout: fixed;
    	width: 70%;
    	float: left;
    }

    .rightSide {
    	table-layout: fixed;
    	width: 29%;
    	float: right;
    }

    .tableTitle {
    	background-color:LightGray;
    	width: 100%;
    	font-size: 20px;
    	font-weight: bold;
    }

    .blueCell {
    	background-color:#00FFFF;
    }

    .yellowCell {
    	background-color:#FFFF00;
    }

    .greenCell {
    	background-color:#90EE90;
    }


    .deatilTable .title{
    	background-color:LightGray;
    	 text-align: center;
    }


    .leftSide table{
    	table-layout: fixed;
    	width: 100%;
    }

    .leftSide table td{
    	table-layout: auto;
    }

    .rightSide table{
    	table-layout: fixed;
    	width: 100%;
    }

    .rightSide table td{
    	table-layout: auto;
    }

    .detail, .detail table td, .detail table{
    	float: right;
    	border: 0px solid black;
    	width: 100%;

    }

    .detail{
    	width: 50%;
    }
    </style>
</head>
